<div id = "form_login">
  <%= simple_form_for :session, url: login_path, remote: true do |f| %>
    <%= f.label t ".email" %>
    <%= f.error :email %>
    <%= f.email_field :email, class: "form-control" %>
    <%= f.label t ".password" %>
    <%= f.error :password %>
    <%= f.password_field :password, class: "form-control" %>
    <%= f.submit t(".log_in"), class: "btn btn-primary" %>
  <% end %>
</div>
